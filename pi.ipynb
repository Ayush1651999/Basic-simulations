    "import numpy as np\n",
    "import math\n",
    "\n",
    "def f(x):\n",
    "    y = math.sqrt(1/(1-x*x))\n",
    "    return y\n",
    "\n",
    "N=100000\n",
    "\n",
    "a = np.linspace(-1, 1, N)\n",
    "da = a[1]-a[0]\n",
    "\n",
    "pi=0\n",
    "\n",
    "for i in range(1,N-1):\n",
    "#     print(a[i])\n",
    "    pi+=da*f(a[i])\n",
    "    \n",
    "print(pi)"
   ]
  }
